{
  "name": "adonis4-tools",
  "displayName": "AdonisJs 4 Tools",
  "description": "AdonisJs 4 Tools that add typing to imported services using hotkey",
  "author": "Vinicios Clarindo <vinicioslc@outlook.com>",
  "license": "MIT",
  "version": "0.2.12",
  "repository": {
    "type": "git",
    "url": "https://github.com/vinicioslc/adonis4-tools"
  },
  "publisher": "vinicioslc",
  "categories": [],
  "keywords": [
    "multi-root ready"
  ],
  "engines": {
    "vscode": "^1.58.1"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -b",
    "watch": "tsc -b -w",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js",
    "publish": "npx vsce publish -p $VSCE_DEPLOY_TOKEN",
    "_____________________________________________________": "UTILITY SCRIPTS",
    "prepare": "husky install",
    "commit": "npm run cm",
    "cm": "cz",
    "cma": "git add . && cz",
    "ri": "npm run release",
    "rici": "release-it --ci",
    "release": "release-it",
    "release:patch": "release-it patch",
    "release:minor": "release-it minor",
    "release:major": "release-it major"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.3.0",
    "@favoloso/conventional-changelog-emoji": "^0.10.0",
    "@release-it/bumper": "^4.0.0",
    "commitizen": "^4.2.4",
    "commitlint-config-gitmoji": "^2.2.5",
    "concurrently": "^7.1.0",
    "cross-env": "^7.0.3",
    "cz-customizable": "^6.3.0",
    "release-it": "^15.5.0",
    "@types/mocha": "^8.2.2",
    "@types/node": "^12.12.0",
    "@types/vscode": "^1.58.1",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "@vscode/test-electron": "^1.6.1",
    "jest": "^29.2.2",
    "assert": "^2.0.0",
    "eslint": "^7.26.0",
    "glob": "^8.0.3",
    "husky": "^7.0.1",
    "mocha": "^8.4.0",
    "prettier": "^2.7.1",
    "standard-version": "^9.3.1",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "debug": "^4.3.2",
    "n-readlines": "^1.0.1"
  },
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onCommand:adonis4_tools.pick_file"
  ],
  "icon": "media/icon.png",
  "main": "./out/index",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Example configuration",
      "properties": {
        "languageServerExample.maxNumberOfProblems": {
          "scope": "resource",
          "type": "number",
          "default": 100,
          "description": "Controls the maximum number of problems produced by the server."
        },
        "languageServerExample.trace.server": {
          "scope": "window",
          "type": "string",
          "enum": [
            "off",
            "messages",
            "verbose"
          ],
          "default": "off",
          "description": "Traces the communication between VS Code and the language server."
        }
      }
    },
    "commands": [
      {
        "command": "adonis4_tools.pick_file",
        "title": "Adonis 4.1 Use('something')"
      }
    ],
    "keybindings": [
      {
        "command": "adonis4_tools.pick_file",
        "key": "ctrl+shift+a"
      }
    ]
  },
  "favolosoEmoji": {
    "showEmojiPerCommit": true
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    }
  }
}
