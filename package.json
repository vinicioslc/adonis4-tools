{
  "name": "adonis4-tools",
  "description": "Adonis4 Tools that add typing to imported services using hotkey",
  "author": "Vinicios Clarindo <vinicioslc@outlook.com>",
  "license": "MIT",
  "version": "0.2.7",
  "repository": {
    "type": "git",
    "url": "https://github.com/vinicioslc/adonis4-tools"
  },
  "publisher": "vinicioslc",
  "categories": [],
  "keywords": [
    "multi-root ready"
  ],
  "engines": {
    "vscode": "^1.58.1"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -b",
    "watch": "tsc -b -w",
    "test": "echo 'SUCCESS'",
    "publish": "npx vsce publish -p $VSCE_DEPLOY_TOKEN",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/mocha": "^8.2.2",
    "@types/node": "^12.12.0",
    "@types/vscode": "^1.58.1",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "@vscode/test-electron": "^1.6.1",
    "eslint": "^7.26.0",
    "husky": "^7.0.1",
    "mocha": "^8.3.2",
    "standard-version": "^9.3.1",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "n-readlines": "^1.0.1"
  },
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onCommand:adonis4_tools.pick_file"
  ],
  "icon": "media/icon.png",
  "main": "./out/index",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Example configuration",
      "properties": {
        "languageServerExample.maxNumberOfProblems": {
          "scope": "resource",
          "type": "number",
          "default": 100,
          "description": "Controls the maximum number of problems produced by the server."
        },
        "languageServerExample.trace.server": {
          "scope": "window",
          "type": "string",
          "enum": [
            "off",
            "messages",
            "verbose"
          ],
          "default": "off",
          "description": "Traces the communication between VS Code and the language server."
        }
      }
    },
    "commands": [
      {
        "command": "adonis4_tools.pick_file",
        "title": "Adonis 4.1 Use('something')"
      }
    ],
    "keybindings": [
      {
        "command": "adonis4_tools.pick_file",
        "key": "ctrl+shift+a"
      }
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
